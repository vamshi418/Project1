
<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<!-- Latest compiled and minified CSS -->
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
		<!-- jQuery library -->
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
		<!-- Latest compiled JavaScript -->
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
		<title>Registration Page</title>
	</head>
	<body>
		<div class="container-fluid">
    		<img class="home-image" src="sports-faith-background.jpg" alt="first slide" width="1330" height="650">
            		<div class="carousel-caption">
            		<br/><br/>
						<div><center><br/>
						<div class="page-header">
							<h2><center>Registration</center><h2>
						</div>
							<div class="rows">
								<div class="form-group">
									<div class="col-md-6"><label for="fname">First Name:</label></div class="col-md-6">
									<div class="col-md-5"><input type="text" class="form-control" placeholder="Enter First Name Here.." ></div class="col-md-5">
  								</div>
							</div><br><br>
							<div class="rows">
								<div class="form-group">
									<div class="col-md-6"><label for="lname">Last Name:</label></div class="col-md-6">
									<div class="col-md-5"><input type="text" class="form-control" placeholder="Enter Last Name Here.." ></div class="col-md-5">								</div>
							</div><br><br>
							<div class="rows">
								<div class="form-group">
									<div class="col-md-6"><label for="uname">User Name:</label></div class="col-md-6">
									<div class="col-md-5"><input type="uname" class="form-control" placeholder="Enter User Name Here.."></div class="col-md-5">
								</div>
							</div><br><br>
							<div class="rows">
								<div class="form-group">
									<div class="col-md-6"><label for="pwd">Password:</label></div class="col-md-6">
									<div class="col-md-5"><input type="password" class="form-control" placeholder="Enter Password Here.."></div class="col-md-5">
								</div>
							</div><br><br>
							<div class="rows">
								<div class="form-group">
									<div class="col-md-6"><label for="email">Email:</label></div class="col-md-6">
									<div class="col-md-5"><input type="text" class="form-control" placeholder="Enter Your Email Id Here.."></div class="col-md-5">
								</div>
							</div><br><br>
							<div class="rows">
								<div class="form-group">
									<div class="col-md-6"><label for="add">Address:</label></div class="col-md-6">
									<div class="col-md-5"><input type="text" class="form-control" placeholder="Enter Address Here.."></div class="col-md-5">
								</div>
							</div><br><br>
							<div class="rows">
								<div class="form-group">
									<div class="col-md-6"><label for="phno">Phone Number:</label></div class="col-md-6">
									<div class="col-md-5"><input type="text" class="form-control" placeholder="Enter Your Mobile Number Here.."></div class="col-md-5">
								</div>
							</div><br><br>
							<div class="rows"><center>
								<class="checkbox"><label><input type="checkbox"> Remember me</label>
							</center></div><br><br>
							<div class="rows"><center>
								<button type="submit" class="btn btn-default"><a href="login.jsp">Submit</a></button>
							</center></div>
						</center>
					</div>
				</div>
		
		</div>
	</body>
</html>